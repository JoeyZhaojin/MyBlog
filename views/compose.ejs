<%- include("partial/header"); -%>

<h1>Compose</h1>

<form action="\compose" method="post">
    <div class="form-group"> 
    <label for="Title">Title</label>
    <input class="form-control" type="text" name="postTitle">
    </div>
    <div class="form-group"> 
    <label for="Post">Post</label>
    <textarea class="form-control" name="postBody" id="" cols="30" rows="10"></textarea>
    </div>
    <button type="submit" class="btn btn-primary" name="button">Publish</button>
</form>

<script src="/Myblog/app.js"></script>





